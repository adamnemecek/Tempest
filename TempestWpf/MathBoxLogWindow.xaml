<Window x:Class="TempestWpf.MathBoxLogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:TempestWpf"
        Title="MathBoxLog" Height="300" Width="300">
    <DockPanel>
        <Grid DockPanel.Dock="Bottom">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button Name="pageLeftButton">&lt;&lt;</Button>
                <Button Name="leftButton">&lt;</Button>
                <Button Name="rightButton">&gt;</Button>
                <Button Name="pageRightButton">&gt;&gt;</Button>
            </StackPanel>
        </Grid>
        <UniformGrid Columns="2">
            <local:MathBoxLogEntryControl x:Name="logEntry1"/>
            <local:MathBoxLogEntryControl x:Name="logEntry2"/>
        </UniformGrid>
    </DockPanel>
</Window>
